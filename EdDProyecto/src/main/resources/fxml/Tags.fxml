<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TagsController" >
    <children>
        <ListView fx:id="listTags" layoutX="100.0" layoutY="50.0" prefHeight="300.0" prefWidth="400.0" />
        <Button fx:id="btnAdd" layoutX="100.0" layoutY="360.0" mnemonicParsing="false" onAction="#btnPress" text="Agregar" />
        <Button fx:id="btnRemove" layoutX="200.0" layoutY="360.0" mnemonicParsing="false" onAction="#btnPress" text="Eliminar" />
    </children>
</AnchorPane>
